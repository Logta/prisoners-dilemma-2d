{
  "name": "prisoners-dilemma-2d-web",
  "version": "0.1.0",
  "type": "module",
  "description": "2D Prisoner's Dilemma Simulation - Web Frontend",
  "scripts": {
    "dev": "vite",
    "build": "npm run copy-wasm && tsc && vite build",
    "copy-wasm": "cp -r ../wasm/pkg/* src/assets/pkg/",
    "preview": "vite preview",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:init": "biome init",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jotai": "^2.6.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.3",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/bun": "1.2.18",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.0.0",
    "vite": "^7.0.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/2d-prisoners-dilemma.git"
  },
  "keywords": [
    "prisoners-dilemma",
    "simulation",
    "genetic-algorithm",
    "evolution",
    "react",
    "typescript",
    "webassembly",
    "rust"
  ],
  "author": "Your Name",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}